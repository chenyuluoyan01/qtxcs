/**modal**/
.v-modals{
  width: 100vw;
  height: 100vh;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 9999999999999999999999;
}
.v-modal{
  width: 100vw;
  height: 100vh;
  position: fixed;
  top: 0;
  left: 0;
  background: rgba(0,0,0,.6);
  z-index: 999999999999999999999;
}
.v-modal .middle{
  height:80vh;
  overflow-y: auto;
  padding-bottom: 60px;
}
.v-modal .modal-lib{
  background:#fff;
  border-radius: 8px;
  width:60%;
  margin:0 auto;
  margin-top:10vh;
  height:80vh;
  position:relative;
  overflow-y: hidden;
  z-index: 9999999999999999999999;
}
.v-modal .modal-lib h4{
  line-height:40px;
  font-size: 14px;
  background:#F8F8F8;
  text-indent: 10px;
  position: relative;
}
.v-modal .modal-lib h4 .close{
  position: absolute;
  right:20px;
  cursor:pointer;
}
.v-modal .modal-lib .bottom{
  width:100%;
  background: #F8F8F8;
  padding: 10px 20px;
  position: absolute;
  bottom:0;
}
.v-modal .modal-lib .bottom .info-btn{
  width:80px;
  float: right;
  margin-right: 20px;
}
